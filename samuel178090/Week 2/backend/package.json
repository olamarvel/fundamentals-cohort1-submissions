  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
