{
  "name": "latency-detective-be",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start:unoptimized": "npm run build && node dist/app.js",
    "start:optimized": "npm run build && node dist/app-optimized.js",
    "profile:unoptimized": "node --inspect dist/app.js",
    "profile:optimized": "node --inspect dist/app-optimized.js",
    "dev:unoptimized": "nodemon --exec \"npm run build && node dist/app.js\"",
    "dev:optimized": "nodemon --exec \"npm run build && node dist/app-optimized.js\""
  },
  "keywords": [
    "performance",
    "worker-threads",
    "nodejs",
    "express",
    "optimization"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "autocannon": "^7.12.0"
  }
}