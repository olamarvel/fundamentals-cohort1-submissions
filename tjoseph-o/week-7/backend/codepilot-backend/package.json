{
  "name": "codepilot-backend",
  "version": "1.0.0",
  "description": "CodePilot Backend API with comprehensive test coverage",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest --verbose --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=integration",
    "test:e2e": "cross-env NODE_ENV=test jest --testPathPattern=e2e",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --coverageReporters=text-lcov --coverageReporters=html",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "testing",
    "ci-cd",
    "express",
    "nodejs"
  ],
  "author": "CodePilot Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }

}
